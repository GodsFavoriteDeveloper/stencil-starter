{
  "name": "stencil-tailwind-starter",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil + Tailwind Starter",
  "scripts": {
    "build": "cross-env NODE_ENV=production stencil build",
    "lint": "eslint src/*/**/*.ts[x]",
    "serve": "stencil build --dev --watch --serve",
    "test": "stencil test --spec",
    "test:watch": "stencil test --spec --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^1.3.3",
    "@stencil/router": "^1.0.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@stencil/eslint-plugin": "^0.2.1",
    "@stencil/postcss": "^1.0.1",
    "@types/autoprefixer": "^9.6.1",
    "@types/cssnano": "^4.0.0",
    "@types/jest": "^24.0.24",
    "@types/node": "^12.12.21",
    "@typescript-eslint/parser": "^2.12.0",
    "autoprefixer": "^9.7.3",
    "cross-env": "^6.0.3",
    "cssnano": "^4.1.10",
    "eslint": "^6.7.2",
    "eslint-plugin-react": "^7.17.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "tailwindcss": "^1.1.4"
  },
  "license": "MIT"
}
