{
  "name": "stencil-tailwind-starter",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil + Tailwind Starter",
  "scripts": {
    "build": "rimraf -f www && cross-env NODE_ENV=production stencil build",
    "lint": "eslint src/*/**/*.ts[x]",
    "serve": "stencil build --dev --watch --serve",
    "test:unit": "stencil test --spec",
    "test:e2e": "stencil test --e2e",
    "test:watch": "stencil test --spec --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^1.12.4",
    "@stencil/router": "^1.0.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.2",
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/postcss": "^1.0.1",
    "@types/autoprefixer": "^9.7.2",
    "@types/cssnano": "^4.0.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@types/puppeteer": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "autoprefixer": "^9.7.6",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "jest": "24",
    "jest-cli": "24",
    "puppeteer": "^3.0.0",
    "puppeteer-core": "^3.0.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^1.2.0",
    "typescript": "^3.8.3"
  },
  "license": "MIT"
}
